{
	"name": "nexus-example",
	"version": "0.1.0",
	"description": "Example SvelteKit application demonstrating Nexus admin framework",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test": "vitest",
		"example:demo": "tsx src/examples/demo.ts",
		"example:startup": "tsx src/examples/startup.ts",
		"example:enterprise": "tsx src/examples/enterprise.ts",
		"examples": "npm run example:demo"
	},
	"dependencies": {
		"@nexus/core": "file:../nexus-core"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^6.1.0",
		"@sveltejs/kit": "^2.37.1",
		"@sveltejs/vite-plugin-svelte": "^6.1.4",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/vite": "^4.1.13",
		"@types/node": "^24.3.1",
		"eslint": "^9.35.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.12.2",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"svelte": "^5.38.7",
		"svelte-check": "^4.3.1",
		"tailwindcss": "^4.1.13",
		"tsx": "^4.20.5",
		"typescript": "^5.9.2",
		"typescript-eslint": "^8.42.0",
		"vite": "^7.1.4",
		"vitest": "^3.2.4"
	},
	"engines": {
		"node": ">=20"
	}
}